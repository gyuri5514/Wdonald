<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wdelivery.qna.dao.QnaDAO">
	<select id="storeSelect" resultType="AdminVO">
		<![CDATA[
		select * from admin where store_address like (concat('%',#{store_address},'%'))
		]]>
	</select>
	<insert id="qnaInsert">
		insert into qa
		values(default, #{qa_title}, #{qa_content}, #{qa_password}, now(), #{qa_status}, #{qa_store}, #{qa_name},
		#{qa_phone1}, #{qa_phone2}, #{qa_phone3}, #{qa_email}, #{qa_agree1}, #{qa_agree2})
	</insert>
	
	
	<select id="qnaSelect" resultType="com.wdelivery.qna.vo.QnaVO">
		select * from qa, qaa where qa.qa_seq = qaa.qa_seq and qa_email = #{qa_email} and qa_password = #{qa_password}
	</select>
	
</mapper>