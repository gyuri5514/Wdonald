<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wdelivery.admin.dao.AdminDAO">
	<select id="indexView" resultType="paymentVO" >
		select order_date,order_status,final_price,payment_type,admin.store_name from order_list, admin
		where admin.store_code = order_list.store_code
	</select>
	
	<select id="userSelect" resultType="userVO">
		select * from user_info
	</select>
	
	<select id="getUserContent" resultType="userVO">
		SELECT * FROM user_info WHERE user_seq = #{user_seq}
	</select>
</mapper>